     select.remove(i);
}
	
	for(var i=0;i<data.length;i++) { 
		
		    option = document.createElement('option');
            option.value =data[i].power ;
            option.text = data[i].power ;
            select.appendChild(option);
		 }
	
     document.getElementById("demo").innerHTML = data;
    }
  };
